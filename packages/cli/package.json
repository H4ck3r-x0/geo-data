{
  "name": "geo-data",
  "version": "0.1.0",
  "description": "Copy only the countries you need. No bloat.",
  "type": "module",
  "files": [
    "dist",
    "LICENSE"
  ],
  "bin": {
    "geo-data": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "geo",
    "countries",
    "cities",
    "i18n",
    "multilingual",
    "cli",
    "geography",
    "typescript",
    "codegen",
    "states",
    "regions",
    "localization",
    "l10n"
  ],
  "author": "Mohammed Fahad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/H4ck3r-x0/geo-data.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/H4ck3r-x0/geo-data#readme",
  "bugs": {
    "url": "https://github.com/H4ck3r-x0/geo-data/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "figlet": "^1.8.0",
    "fs-extra": "^11.2.0",
    "gradient-string": "^3.0.0",
    "prompts": "^2.4.2",
    "string-similarity": "^4.0.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^20.11.0",
    "@types/prompts": "^2.4.9",
    "@types/string-similarity": "^4.0.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
